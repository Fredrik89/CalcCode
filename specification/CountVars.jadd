aspect CountVars {
	public ArrayList<Variable> Procedure.countVars(ArrayList<Variable> vars, int blockLevel){
		int variableIndex = 0;
		for(Stmt stmt : getStmtList()){
			if(stmt instanceof IdDecl){
				vars.add(new Variable(blockLevel, variableIndex));
				variableIndex++;
			} 
		}
		return vars;
	}
}